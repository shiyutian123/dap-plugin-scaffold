{
    "name": "dcloud-digitalize-platform",
    "version": "0.0.0",
    "scripts": {
      "ng": "ng",
      "start": "ng serve -o --port=4800",
      "hmr": "ng serve -c=hmr --port=4800",
      "build": "ng build --base-href ./",
      "build-prod": "ng build --prod --build-optimizer --base-href ./",
      "analyze": "ng build --prod --build-optimizer --stats-json",
      "lint": "npm run lint:ts && npm run lint:style",
      "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
      "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
      "lint-staged": "lint-staged",
      "e2e": "ng e2e",
      "test": "ng test --watch",
      "test-coverage": "ng test --code-coverage --watch=false",
      "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -r 4848"
    },
    "private": true,
    "dependencies": {
      "@angular/animations": "~7.1.0",
      "@angular/cdk": "^7.1.0",
      "@angular/common": "~7.1.0",
      "@angular/compiler": "~7.1.0",
      "@angular/core": "~7.1.0",
      "@angular/forms": "~7.1.0",
      "@angular/platform-browser": "~7.1.0",
      "@angular/platform-browser-dynamic": "~7.1.0",
      "@angular/router": "~7.1.0",
      "@ngx-progressbar/core": "^5.3.1",
      "@ngx-progressbar/http": "^5.3.1",
      "@ngx-progressbar/router": "^5.3.1",
      "@swimlane/dragula": "^3.7.3",
      "@swimlane/ngx-dnd": "^6.0.0",
      "@types/dragula": "^2.1.34",
      "classlist.js": "^1.1.20150312",
      "core-js": "^2.5.4",
      "editorconfig-tools": "^0.1.1",
      "hammerjs": "^2.0.8",
      "jquery": "3",
      "lint-staged": "^8.1.0",
      "ng-zorro-antd": "^7.0.0-rc.1",
      "ngx-progressbar": "^2.1.1",
      "rxjs": "~6.3.3",
      "tslib": "^1.9.0",
      "web-animations-js": "^2.3.1",
      "zone.js": "~0.8.26"
    },
    "devDependencies": {
      "@angular-devkit/build-angular": "~0.11.0",
      "@angular/cli": "~7.1.4",
      "@angular/compiler-cli": "~7.1.0",
      "@angular/language-service": "~7.1.0",
      "@angularclass/hmr": "^2.1.3",
      "@compodoc/compodoc": "^1.1.7",
      "@types/jasmine": "~2.8.8",
      "@types/jasminewd2": "~2.0.3",
      "@types/node": "~8.9.4",
      "codelyzer": "~4.5.0",
      "husky": "^1.3.0",
      "jasmine-core": "~2.99.1",
      "jasmine-spec-reporter": "~4.2.1",
      "karma": "~3.1.1",
      "karma-chrome-launcher": "~2.2.0",
      "karma-coverage-istanbul-reporter": "~2.0.1",
      "karma-jasmine": "~1.1.2",
      "karma-jasmine-html-reporter": "^0.2.2",
      "prettier": "^1.15.3",
      "prettier-stylelint": "^0.4.2",
      "protractor": "~5.4.0",
      "stylelint": "^9.9.0",
      "stylelint-config-standard": "^18.2.0",
      "ts-node": "~7.0.0",
      "tslint": "~5.11.0",
      "typescript": "~3.1.6"
